                                    <MenuItem id="saveConfigMenuItem" fx:id="saveConfigMenuItem" mnemonicParsing="false" text="Save Config" />
                                    <MenuItem id="loadConfigMenuItem" fx:id="loadConfigMenuItem" mnemonicParsing="false" text="Load Config" />
