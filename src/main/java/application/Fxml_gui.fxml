                            </Menu>
                            <Menu id="configMenu" fx:id="configMenu" mnemonicParsing="false" text="Config">
                              <items>
                                <MenuItem id="setConfigMenuItem" fx:id="setConfigMenuItem" mnemonicParsing="false" text="Set Config" />
                                    <MenuItem id="resetConfigMenuItem" fx:id="resetConfigMenuItem" mnemonicParsing="false" text="Reset Config" />
                              </items>
                            </Menu>
                              <Menu id="optionsMenu" fx:id="optionsMenu" mnemonicParsing="false" text="Options">
